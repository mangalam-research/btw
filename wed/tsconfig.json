{
  "compilerOptions": {
    "module": "amd",
    "moduleResolution": "node",
    "strict": true,
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipDefaultLibCheck": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "outDir": "",
    "sourceMap": true,
    "inlineSources": true,
    "target": "es6",
    "lib": ["dom", "es2015"]
  }
}
